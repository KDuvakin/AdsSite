
@{
    ViewBag.Title = "AddfromCSV";
}
<h2>Add From CSV</h2>
@using (Html.BeginForm("AddfromCSV", "Admin", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" }))
{
    <div class="row">
        <div class="col-lg-2 col-md-2 col-sm-3 hidden-xs">
            <ul class="categories">
                <li><a href="@Url.Action("UserControl","Admin")">User Control</a></li>
                <li><a href="@Url.Action("ManageAds","Admin")">Managing Ads</a></li>
                <li><a href="@Url.Action("AllAdsControl","Admin")">View All Ads</a></li>
            </ul>
        </div>
        <div class="col-lg-10 col-md-10">
            <div class="control-label col-md-2">
                CSV:
            </div>
            <div class="col-md-12 col-lg-12 col-xs-12">
                <input type="file" id="FileUpload" name="FileUpload"  class=""   />
            </div>
            <div class="form-group buttons-bottom">
                <div class="col-lg-3 col-xs-4">
                    <a class="btn btn-lg btn-warning btn-width" href="@Url.Action("ViewDiscription","Admin", new { @AdvertSubgroupe = Model })">Back </a>
                </div>
                <div class="ccol-lg-3 col-xs-4">
                    <input type="submit" id="btnSubmit" class="btn btn-lg btn-success btn-width" value="Add from CSV" />
                </div>
            </div>
        </div>
    </div>

}

